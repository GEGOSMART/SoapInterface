                   encodingStyle="http://schemas.xmlsoap.org/soap/encoding/"/>
      </wsdl:input>
      <wsdl:output>
       <soap:body namespace="http://localhost:2000/questionsrng"
                  use="literal"
                  encodingStyle="http://schemas.xmlsoap.org/soap/encoding/"/>
     </wsdl:output>
    </wsdl:operation>

  </wsdl:binding>

  <wsdl:service name="ws">
    <wsdl:port name="questionList" binding="tns:questionsBinding">
      <soap:address location="http://localhost:2000/questionsrng"/>
    </wsdl:port>
  </wsdl:service>
