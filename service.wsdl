<?xml version="1.0" encoding="UTF-8"?>
<wsdl:definitions name="questionserv" targetNamespace="http://localhost:2000/questionserv" xmlns="http://localhost:2000/questionserv" xmlns:http="http://schemas.xmlsoap.org/wsdl/http/" xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/" xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/" xmlns:xs="http://www.w3.org/2001/XMLSchema">

  <wsdl:message name="questionsRequest">
    <wsdl:part name="category" type="xs:string"></wsdl:part>
  </wsdl:message>


  <wsdl:message name="questionsResponse">
    <wsdl:part name="res" type="xs:string"></wsdl:part>
  </wsdl:message>


  <wsdl:portType name="questionsPort">
    <wsdl:operation name="questions">
      <wsdl:input message="questionsRequest"></wsdl:input>
      <wsdl:output message="questionsResponse"></wsdl:output>
    </wsdl:operation>
   </wsdl:portType>

  <wsdl:binding name="questionsBinding" type="questionsPort">
    <soap:binding style="rpc"
                  transport="http://schemas.xmlsoap.org/soap/http"/>
    <wsdl:operation name="questions">
      <soap:operation soapAction="questions"/>
      <wsdl:input>
        <soap:body use="literal"
                   encodingStyle="http://schemas.xmlsoap.org/soap/encoding/"/>
      </wsdl:input>
    </wsdl:operation>

  </wsdl:binding>

  <wsdl:service name="ws">
    <wsdl:port name="questionList" binding="questionsBinding">
      <soap:address location="http://localhost:2000/questions"/>
    </wsdl:port>
  </wsdl:service>
</wsdl:definitions>
